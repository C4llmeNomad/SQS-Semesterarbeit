@article{Turing_Test_noTrick,
	author = {Harnad, Stevan},
	title = {The Turing Test is Not a Trick: Turing Indistinguishability is a Scientific Criterion},
	year = {1992},
	issue_date = {Oct. 1992},
	publisher = {Association for Computing Machinery},
	address = {New York, NY, USA},
	volume = {3},
	number = {4},
	issn = {0163-5719},
	url = {https://doi.org/10.1145/141420.141422},
	doi = {10.1145/141420.141422},
	abstract = {It is important to understand that the Turing Test (TT) is not, nor was it intended
	to be, a trick; how well one can fool someone is not a measure of scientific progress.
	The TT is an empirical criterion: It sets AI's empirical goal to be to generate human
	scale performance capacity. This goal will be met when the candidate's performance
	is totally indistinguishable from a human's. Until then, the TT simply represents
	what it is that AI must endeavor eventually to accomplish scientifically.},
	journal = {SIGART Bull.},
	month = oct,
	pages = {9–10},
	numpages = {2}
}

